{
  "name": "showcase-hn-comments-semantic-search",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "parcel serve index.html --port 3001",
    "indexer": "node scripts/convertPostgres.js && node scripts/indexDataInTypesense.js",
