    "jquery": "^3.7.0",
    "pg": "^8.11.5",
    "typesense": "^1.7.2-3",
    "typesense-instantsearch-adapter": "^2.8.0-0"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.8.3",
    "buffer": "^5.5.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prettier": "5.0.0",
    "parcel": "^2.8.3",
    "prettier": "3.0.1",
    "process": "^0.11.10"
  }
